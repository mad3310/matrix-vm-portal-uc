<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.letvcloud.uc.mapper.CountryMapper">

    <resultMap id="countryResultMap" type="country">

    </resultMap>


    <select id="getCountryList" resultMap="countryResultMap">
       SELECT
          countryName,
          countryDomainName,
          countryAreaNum
        FROM
	      uc_country
    </select>


</mapper>